{
	"name": "vuesoma",
	"version": "0.1.2-1",
	"private": true,
	"scripts": {
		"serve": "echo 'use `npm start` to select a project to serve",
		"serve:assisted-channels": "npx cross-env VUE_APP_CURRENT_PROJECT=assisted-channels vue-cli-service serve",
		"serve:bancofar": "npx cross-env VUE_APP_CURRENT_PROJECT=bancofar vue-cli-service serve",
		"serve:caminos": "npx cross-env VUE_APP_CURRENT_PROJECT=caminos vue-cli-service serve",
		"build:assisted-channels": "npx cross-env VUE_APP_CURRENT_PROJECT=assisted-channels vue-cli-service build",
		"build:bancofar": "npx cross-env VUE_APP_CURRENT_PROJECT=bancofar vue-cli-service build",
		"build:caminos": "npx cross-env VUE_APP_CURRENT_PROJECT=caminos vue-cli-service build",
		"lint": "cross-env VUE_APP_CURRENT_PROJECT=bancofar vue-cli-service lint",
		"docs:serve": "vuepress dev docs",
		"docs:build": "vuepress build docs",
		"start": "node commands/index.js",
		"test:e2e": "'use `npm start` to select a project to serve",
		"test:e2e:bancofar": "npx cross-env VUE_APP_CURRENT_PROJECT=bancofar vue-cli-service test:e2e",
		"test:e2e:caminos": "npx cross-env VUE_APP_CURRENT_PROJECT=caminos vue-cli-service test:e2e",
		"test:unit": "npx cross-env VUE_APP_CURRENT_PROJECT=caminos VUE_APP_ENABLE_COVERAGE=true vue-cli-service test:unit",
		"test:unit:watch": "npx cross-env VUE_APP_CURRENT_PROJECT=caminos vue-cli-service test:unit --watchAll",
		"test:unit:watch:coverage": "npx cross-env VUE_APP_CURRENT_PROJECT=caminos VUE_APP_ENABLE_COVERAGE=true vue-cli-service test:unit --watchAll",
		"debug:test:unit": "npx cross-env VUE_APP_CURRENT_PROJECT=bancofar node --inspect-brk ./node_modules/@vue/cli-service/bin/vue-cli-service.js test:unit --runInBand",
		"debug:test:unit:watch": "npx cross-env VUE_APP_CURRENT_PROJECT=bancofar node --inspect-brk ./node_modules/@vue/cli-service/bin/vue-cli-service.js test:unit --runInBand --watchAll",
		"check:cmp:dep": "node ./commands/dependenciesChecker.js",
		"check:locales": "node ./commands/localesChecker.js",
		"audit:bancofar": "node commands/auditProject bancofar",
		"audit:caminos": "node commands/auditProject caminos"
	},
